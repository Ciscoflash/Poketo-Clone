            //re-click firing
            $(document).ready(function(){
              //checkout button click
              $(document).on('click', '.nvd-checkout', function(event){
                  event.preventDefault();
                  if($(document).find('#shippingProtectionCheckBox').is(':checked')){
                    var data_protected_variant = $(document).find('#shippingProtectionCheckBox').attr('data-protected-variant');
                    addShippingProtection(data_protected_variant);
                  }else{
                    window.location.href = '/checkout';
                  }
              });

              //on update drawer
                $(document).on('click','.quantity__button', function(){
                  console.log('Click happened');
                  setTimeout(nvd_init(2),1500);
                });
              //cart-remove-button
                $(document).on('click','.cart__remove__icon', function(){
                  console.log('Click happened');
                   setTimeout(nvd_init(2),1500);
                });

                $('.btn--add-to-cart').on('click', function(){
                  console.log('Click happened');
                  setTimeout(nvd_init(2),1500);
                });

              //cart count

              var current_count = '0';
              current_count = parseInt(current_count);
              
              
              var nvdFinder = '0'
              current_count = current_count-parseInt(nvdFinder);
              $(document).find('.cart-count-bubble span').text(current_count);
            })

            const observer = new PerformanceObserver((list) => {
              for (const entry of list.getEntries()) {
                if (entry.initiatorType === "fetch" || entry.initiatorType === "xmlhttprequest") {


                  
                  if(entry.name.includes('/cart/change.js')){
                    console.log('cart change request found');
                   setTimeout(nvd_init(2),1500);
                  }
                }
              }
            });

    observer.observe({
      entryTypes: ["resource"]
    });
</script>

